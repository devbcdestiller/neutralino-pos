{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/src/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "Hello Neutralinojs",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/src/public/vite.svg"
    }
  },
  "cli": {
    "binaryName": "neutralinojs-zero",
    "resourcesPath": "/src/dist",
    "binaryVersion": "5.2.0",
    "frontendLibrary": {
      "patchFile": "/src/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/src/",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}